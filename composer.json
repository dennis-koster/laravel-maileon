{
    "name": "dennis-koster/laravel-maileon",
    "type": "library",
    "description": "Maileon mail driver for Laravel",
    "keywords": [
        "dennis-koster",
        "laravel-maileon",
        "laravel",
        "maileon",
        "mail-driver"
    ],
    "homepage": "https://github.com/denniskoster/laravel-maileon",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Koster",
            "email": "denniskoster90@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "bensampo/laravel-enum": "^3.4",
        "laravel/framework": "^8.0",
        "php-http/discovery": "^1.14",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^2.0",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^6.19",
        "phpstan/phpstan-mockery": "^0.12.13",
        "phpstan/phpstan-phpunit": "^0.12.18",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DennisKoster\\LaravelMaileon\\Providers\\LaravelMaileonServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "DennisKoster\\LaravelMaileon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DennisKoster\\LaravelMaileon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "coverage": "vendor/bin/phpunit",
        "test": "vendor/bin/phpunit --no-coverage"
    }
}
